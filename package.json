{
  "name": "back-twa-beta",
  "version": "0.0.0",
  "description": "An app for Trade With Africa, a startup by Annick Thiombiano",
  "engines": {
    "node": "12.x"
  },
  
  "main": "index.js",
  "private": true,
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node app.js",
    "start:dev": "concurrently \"nodemon --ignore 'front-twa-beta/*'\" \"npm run front-twa-beta\"",
    "front-twa-beta": "cd front-twa-beta && npm run start",
    "install": "cd front-twa-beta && npm install",
    "build": "cd front-twa-beta && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "morgan": "~1.9.1",
    "ejs": "^2.5.6",
    "axios": "^0.19.0",
    "bcrypt": "^3.0.2",
    "body-parser": "^1.18.3",
    "dotenv": "^8.0.0",
    "express-fileupload": "^1.0.0",
    "if-env": "^1.0.4",
    "jsonwebtoken": "^8.3.0",
    "mongoose": "^5.6.1",
    "multer": "^1.4.1",
    "react-modal": "^3.6.1"
  },
  "devDependencies": {
    "got": "^11.3.0",
    "tape": "^4.7.0",
    "concurrently": "^4.1.0",
    "nodemon": "^1.11.0"
  },
  "author": "Alison Stuart (sedulous-mortal)",
  "license": "MIT"
}
